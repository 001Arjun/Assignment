@model IEnumerable<Assignment.Models.ExCategory>


@{
    ViewBag.Title = "Dashboard";
    @*Layout = "~/Views/Shared/_Layout.cshtml";*@
}



<h2 align="center">Dashboard</h2>

<h3>Category List</h3>


@if (TempData["AlertMsg"] != null)
{
    <div class="alert alert-success">
        <strong>Success !</strong>@TempData["AlertMsg"]
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
    </div>
}


@*<p>
        @Html.ActionLink("Create New", "Create")

    </p>*@

<table class="table" border='1'>
    <tr style="background-color: orangered; color: white">
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ex_Limit)
        </th>
      

    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@Html.ActionLink(item.Name, "CatNameSearch", "ExCategory", new { Id = item.Id }, null)</td>
    <td>@item.Ex_Limit</td>
    
</tr>

    }
</table>



